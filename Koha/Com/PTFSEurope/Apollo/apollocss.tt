}

#dwt-search-widget br {
    display: none;
}

#dwt-search-widget #full-text-div label {
    display: block;
}

.mastheadsearch select,
.mastheadsearch textarea,
.mastheadsearch input[type="text"],
.mastheadsearch input[type="password"],
.mastheadsearch input[type="datetime"],
.mastheadsearch input[type="datetime-local"],
